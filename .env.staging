# Staging Environment Variables
# Uses Sepolia testnets for all chains

# Enable testnet mode - uses Sepolia chains and staging services
VITE_TESTNET_MODE=true

# Backend API URL (staging - runs on port 3002)
VITE_API_URL=http://localhost:3002/api

# WalletConnect Project ID (get from https://cloud.walletconnect.com)
# Use a separate project ID for staging if desired
VITE_WALLETCONNECT_PROJECT_ID=

# TheGraph API Key (optional - for Uniswap pool data)
VITE_THEGRAPH_API_KEY=

# Relayr Staging
VITE_RELAYR_APP_ID=43a6827c-3407-43c1-89c6-deeb8994696d
VITE_RELAYR_API_KEY=0717579b-c72a-4da8-a7ba-c95944db4e7f
